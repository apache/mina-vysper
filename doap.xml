        <homepage rdf:resource='https://mina.apache.org/vysper-project/'/>
        <download-page rdf:resource='https://mina.apache.org/vysper-project/downloads.html'/>
        <bug-database rdf:resource='http://issues.apache.org/jira/browse/VYSPER'/>
        <license rdf:resource='https://github.com/apache/mina-vysper/blob/master/LICENSE.txt'/>
